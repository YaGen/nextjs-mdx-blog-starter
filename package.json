{
  "name": "nextjs-blog-starter-pack",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "now-build": "next build && next export"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.5.0",
    "@mdx-js/mdx": "^1.5.0",
    "@next/mdx": "^9.0.7",
    "next": "^9.3.2",
    "react": "16.10.1",
    "react-dom": "16.10.1"
  },
  "devDependencies": {
    "babel-plugin-preval": "^3.0.1",
    "husky": "^3.0.8",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.mdx?": [
      "prettier --write --parser mdx",
      "git add"
    ]
  }
}
